 <link rel="stylesheet" href="/stylesheets/tracks.css">

<div style="margin-left:50px">
  <h1>Tracks</h1>
</div>

<% @tracks.each do |track| %>
  <div class="track">
    <h3><%= track.title %> <small>(by: <%= track.author %>)</small></h3>
    <p>
      <%= track.content %>
    </p>
  </div>
<% end %>